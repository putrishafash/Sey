<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shafaseyo.bikesrents">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SplashActivity" android:theme="@style/AppTheme"/>
        <activity android:name="com.shafaseyo.bikesrents.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.shafaseyo.bikesrents.activity.DaftarSepedaActivity"
            android:label="@string/data_sepeda"
            android:parentActivityName=".activity.MainActivity"/>
        <activity android:name="com.shafaseyo.bikesrents.activity.PenyewaActivity"
            android:label="@string/penyewa"
            android:parentActivityName=".activity.MainActivity"/>
        <activity android:name="com.shafaseyo.bikesrents.activity.SewaSepedaActivity" />
        <activity android:name="com.shafaseyo.bikesrents.activity.DetailSepedaActivity"
            android:label="@string/data_sepeda"
            android:parentActivityName=".activity.MainActivity"/>
        <activity android:name="com.shafaseyo.bikesrents.activity.DetailPenyewaActivity"
            android:label="@string/penyewa"
            android:parentActivityName=".activity.MainActivity"/>
        <activity android:name=".activity.About"
            android:label="About"
            android:parentActivityName=".activity.About"/>
    </application>

</manifest>